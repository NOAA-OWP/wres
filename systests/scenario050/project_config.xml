<?xml version="1.0" encoding="UTF-8"?>
<project label="Single Valued CSV" name="inDevelopmentScenario050">
    <inputs>
        <left label="">
            <type>simulations</type>
            <source format="PI-XML">../data/DRRC2QINE.xml</source>
            <variable>QINE</variable>
        </left>
        <right label="">
            <type>single valued forecasts</type>
            <source format="csv" has_header="true">../smalldata/DRRC2_SQIN_FAKE.csv</source>
            <variable>SQIN</variable>
        </right>
    </inputs>
    <pair label="Pairs by location and lead time">
        <unit>CMS</unit>
    </pair>
    <metrics>
        <metric><name>mean error</name></metric>
        <metric><name>sample size</name></metric>
    </metrics>
    <outputs>
        <destination type="pairs" decimalFormat="0.000000" />
        <destination type="numeric" decimalFormat="0.000000" />
    </outputs>
</project>

