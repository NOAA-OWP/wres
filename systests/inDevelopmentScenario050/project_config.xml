<?xml version="1.0" encoding="UTF-8"?>
<project label="ExampleProject" name="inDevelopmentScenario050">
    <inputs>
        <left label="">
            <type>simulations</type>
            <source format="PI-XML">../data/DRRC2QINE.xml</source>
            <variable>QINE</variable>
        </left>
        <right label="">
            <type>single valued forecasts</type>
            <source format="csv" >../smalldata/DRRC2_SQIN_FAKE.csv</source>
            <variable>SQIN</variable>
        </right>
    </inputs>
    <pair label="Pairs by location and lead time">
        <unit>CMS</unit>
    </pair>
    <metrics>
        <thresholds>
            <applyTo>left</applyTo>
            <commaSeparatedValues>0.1,0.25,0.5,0.75,0.9,0.95</commaSeparatedValues>
            <operator>greater than or equal to</operator>
        </thresholds>
        <metric><name>all valid</name></metric>
    </metrics>
    <outputs>
        <destination type="pairs" decimalFormat="0.000000">
            <path>output/</path>
        </destination>
        <destination type="numeric" decimalFormat="0.000000">
            <path>output/</path>
        </destination>
        <destination type="graphic">
            <path>output/</path>
            <graphical width="800" height="600">
            </graphical>
        </destination>
    </outputs>
</project>

