<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="wres.Source - Create project table for any dbms."
               author="Jesse Bickel">
        <preConditions>
            <not>
                <tableExists tableName="Project" schemaName="wres" />
            </not>
        </preConditions>
        <createTable tableName="Project" schemaName="wres">
            <column name="project_id" type="INTEGER" autoIncrement="true">
                <constraints primaryKey="true" />
            </column>
            <column name="input_code" type="INTEGER">
                <constraints unique="true" nullable="false" />
            </column>
            <column name="project_name" type="VARCHAR" />
        </createTable>
    </changeSet>
</databaseChangeLog>
