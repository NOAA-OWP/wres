    <changeSet id="MeasurementUnit1.0" author="Christopher Tubbs" dbms="PostgreSQL">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="f">
                SELECT EXISTS (
                    SELECT 1
                    FROM pg_catalog.pg_tables
                    WHERE schemaname = 'wres'
                        AND tablename = 'measurementunit'
                );
            </sqlCheck>
        </preConditions>
        <createTable tableName="MeasurementUnit" schemaName="wres">
            <column name="measurementunit_id" type="smallserial" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="unit_name" type="text">
                <constraints nullable="false" unique="true"/>
            </column>
        </createTable>
        <comment>
            The wres.MeasurementUnit table has been created, along with all default entries.
        </comment>
    </changeSet>
