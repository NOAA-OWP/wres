<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd"
                   objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="wres.SourceCompleted - Create SourceCompleted for any dbms."
               author="Jesse Bickel">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="SourceCompleted" schemaName="wres" />
            </not>
        </preConditions>
        <createTable tableName="SourceCompleted" schemaName="wres">
            <column name="source_id" type="INTEGER">
                <constraints references="wres.Source"
                             foreignKeyName="sourcecompleted_source_fk"
                             deleteCascade="true "/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
