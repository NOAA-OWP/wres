<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="Remove wres.Observation.time_step" author="Christopher Tubbs">
        <preConditions onFail="MARK_RAN">
            <columnExists schemaName="wres" tableName="Observation" columnName="time_step" />
        </preConditions>
        <dropColumn tableName="Observation" columnName="time_step" schemaName="wres" />
        <comment>
            The column wres.Observation.time_step has been removed.
        </comment>
    </changeSet>
</databaseChangeLog>