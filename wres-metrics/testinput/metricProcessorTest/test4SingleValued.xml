<?xml version="1.0" encoding="UTF-8"?>
<project label="ExampleProject">
  <inputs>
    <left label="My Desired Name For Graphics" type="observation">
      <type>observations</type>
      <source recursive="true" all="true" format="PI-XML">/media/sf_userprofile/hefs_data/resources/DRRC2SQIN.xml</source>
      <variable>QINE</variable>
    </left>
    <right label="My Name I want to see on charts">
      <type>single valued forecasts</type>
      <source format="PI-XML">/media/sf_userprofile/hefs_data/resources/forecasts/</source>
      <variable>SQIN</variable>
    </right>
  </inputs>
  <pair label="Pairs by location and lead time"> 
    <unit>CMS</unit>
  </pair>
  <metrics>
    <thresholds>
      <type>value</type>
      <commaSeparatedValues>0.5,0.75,0.83</commaSeparatedValues>
    </thresholds>	
    <metric>
	<name>mean error</name>
    </metric>
    <metric>
	<name>pearson correlation coefficient</name>
    </metric>
    <metric>
	<name>root mean square error</name>
    </metric>
  </metrics>
  <metrics>
    <thresholds>
      <type>value</type>
      <commaSeparatedValues>0.4,0.75,0.9</commaSeparatedValues>
    </thresholds>	
    <metric>
	<name>mean square error</name>
    </metric>
    <metric>
	<name>mean absolute error</name>
    </metric>
  </metrics>
  <outputs>
    <destination type="numeric">/home/jesse/text_output"</destination>
    <destination type="graphic"><path>/tmp/</path>
    </destination>
  </outputs>
</project>

